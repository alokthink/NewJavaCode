<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd"> 
 <hibernate-mapping>
  <class name="bean.Movies" table="movie">
  <id name="mid" />
  <property name="moviesName" />
  <many-to-one name="actor" class="bean.Actor">
   <column name="aid"/>
  </many-to-one>
  </class>
 </hibernate-mapping>