//		array: 1 2 3 4 5
//		K: 3
//		output: 3 2 1 5 4
		
		int [] arr2= {1,2,3,4,5};
		StringBuilder b=new StringBuilder();
		int m=3;
		for(int i=m-1;i>=0;i--) {
			b.append(arr2[i]);
		}
		for(int n=arr2.length-1;n>=m;n--) {
			b.append(arr2[n]);	
		}
		
		System.out.println(b);
			

}
}
