package com.mps.think.model;

public class OrderClass {
	private int ocID;
	private String orderCode;
	private String description;
	private String profitCenter;
	private String parentOc;
	private String ocType;
	public int getOcID() {
		return ocID;
	}
	public void setOcID(int ocID) {
		this.ocID = ocID;
	}
	public String getOrderCode() {
		return orderCode;
	}
	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getProfitCenter() {
		return profitCenter;
	}
	public void setProfitCenter(String profitCenter) {
		this.profitCenter = profitCenter;
	}
	public String getParentOc() {
		return parentOc;
	}
	public void setParentOc(String parentOc) {
		this.parentOc = parentOc;
	}
	public String getOcType() {
		return ocType;
	}
	public void setOcType(String ocType) {
		this.ocType = ocType;
	}
	

}
