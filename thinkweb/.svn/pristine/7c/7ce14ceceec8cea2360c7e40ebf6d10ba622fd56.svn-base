package com.mps.think.model;

public class DropdownModel {
	
	private String key;
	private String display;
	private String description;
	private String extra;
	private String extraData;
	private String extraDataDef;
	private String extraDataDef2;
	
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getExtraDataDef2() {
		return extraDataDef2;
	}
	public void setExtraDataDef2(String extraDataDef2) {
		this.extraDataDef2 = extraDataDef2;
	}
	public String getDisplay() {
		return display;
