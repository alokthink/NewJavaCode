package com.mps.think.process.model;

public class SortModel {
	private String processSort;
	private String description;
	private int mruProcessSortFieldSeq;
	
	private int processSortFieldSeq;
	private String direction;
	private String columnName;
	private String tableName;
	public String getProcessSort() {
		return processSort;
	}
	public void setProcessSort(String processSort) {
		this.processSort = processSort;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getProcessSortFieldSeq() {
		return processSortFieldSeq;
	}
	public void setProcessSortFieldSeq(int processSortFieldSeq) {
		this.processSortFieldSeq = processSortFieldSeq;
	}
	public String getDirection() {
		return direction;
	}
	public void setDirection(String direction) {
		this.direction = direction;
	}
	public String getColumnName() {
		return columnName;
	}
	public void setColumnName(String columnName) {
		this.columnName = columnName;
	}
	public String getTableName() {
		return tableName;
	}
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	public int getMruProcessSortFieldSeq() {
		return mruProcessSortFieldSeq;
	}
	public void setMruProcessSortFieldSeq(int mruProcessSortFieldSeq) {
		this.mruProcessSortFieldSeq = mruProcessSortFieldSeq;
	}
	
	
	

}
